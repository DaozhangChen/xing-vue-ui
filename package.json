    "vue": "3.2.45",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "4.0.0",
    "typescript": "4.9.3",
    "vite": "4.0.0",
    "vite-plugin-dts": "1.7.1",
    "vite-svg-loader": "4.0.0",
    "vue-tsc": "1.0.11"
  }
